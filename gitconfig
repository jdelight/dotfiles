  s = status
  b = branch
  ps = push
  p = pull
  h = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  l = log --oneline
  fa = fetch --all	
  type = cat-file -t
  dump = cat-file -p

