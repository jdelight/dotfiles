[push]
	default = simple
[user]
	name = jdelight
	email = james@jdelight.co.uk

[alias]
  o = checkout
  i = commit
  s = status
  b = branch
  ps = push
  p = pull
  h = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  l = log --oneline
  fa = fetch --all	
  ri = rebase -i
  type = cat-file -t
  dump = cat-file -p

[url "https://"]
	insteadOf = git://
